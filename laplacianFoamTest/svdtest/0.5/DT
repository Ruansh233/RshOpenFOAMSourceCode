FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.5";
    object      DT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 4e-05;

boundaryField
{
    patch1
    {
        type            calculated;
        value           uniform 4e-05;
    }
    patch2
    {
        type            calculated;
        value           uniform 4e-05;
    }
    patch3
    {
        type            calculated;
        value           uniform 4e-05;
    }
    patch4
    {
        type            calculated;
        value           uniform 4e-05;
    }
}


