FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

3
(
    inlet
    {
        type            patch;
        nFaces          2;
        startFace       4;
    }
    outlet
    {
        type            patch;
        nFaces          2;
        startFace       6;
    }
    wall
    {
        type            wall;
        inGroups        1(wall);
        nFaces          12;
        startFace       8;
    }
)

